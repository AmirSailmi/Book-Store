<Window x:Class="WpfApp1.Fund"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Fund" Height="605" Width="400" Background="#FF1B1717" WindowStyle="None" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" TextAlignment="Center" Text="Fund" VerticalAlignment="Top" Margin="55,10,0,0" Height="57" Width="286" Foreground="#FFC4D334" FontSize="36"/>
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" TextAlignment="Center" Text="Total Money:" VerticalAlignment="Top" Margin="55,178,0,0" Height="41" Width="286" Foreground="#FFC4D334" FontSize="25"/>
        <TextBlock x:Name="fundMoney" HorizontalAlignment="Left" TextWrapping="Wrap" TextAlignment="Center" Text="(money)" VerticalAlignment="Top" Margin="55,224,0,0" Height="41" Width="286" Foreground="#FFC4D334" FontSize="25"/>
        <Button x:Name="FundBack" Content="Back" FontSize="12" Background="#FF1B1717" Foreground="#FF95AD29" HorizontalAlignment="Left" VerticalAlignment="Top" Width="89" Margin="30,545,0,0" Height="29" Click="FundBack_Click"/>

    </Grid>
</Window>
